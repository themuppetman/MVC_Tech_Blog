<header>
    {{#if logged_in}}
    <nav>
        <a href="/">Home</a>
        <a href="/dashboard">Dashboard</a>
        <a href="/logout">Logout</a>
    </nav>
    {{else}}
    <nav>
        <a href="/">Home</a>
        <a href="/login">Login</a>
        <a href="/signup">Sign Up</a>
    </nav>
    {{/if}}
</header>
<main>
    {{#each blogs}}
    <article>
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <p>Posted by {{this.user.username}} on {{this.createdAt}}</p>
    </article>
    {{/each}}
</main>